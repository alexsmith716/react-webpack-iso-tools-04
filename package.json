  "scripts": {
    "start-dev": "BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon index.js --ignore client/ --ignore webpack/webpack-assets.json",
    "start-prod": "BABEL_DISABLE_CACHE=1 NODE_ENV=production node index.js",
    "build-start": "yarn clear && yarn build && yarn start-prod",
    "build": "yarn clear && yarn build-client && yarn build-server",
    "build-dev": "yarn clear-dev && webpack --config webpack.config.dev.js",
    "build-client": "yarn clear-client && NODE_ENV=production webpack --config webpack.config.prod.js",
    "build-server": "yarn clear-server && NODE_ENV=production BABEL_DISABLE_CACHE=1 webpack --config webpack.config.server.js",
    "clear": "rimraf build",
    "clear-dev": "rimraf dist",
    "clear-client": "rimraf public/static/dist/client",
    "clear-server": "rimraf build/server",
    "clearr": "rimraf build rimraf dist rimraf public/static/dist/client rimraf build/server"
  },
